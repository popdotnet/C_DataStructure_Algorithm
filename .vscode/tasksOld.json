{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Compile Stack",
            "type": "shell",
            "command": "gcc",
            "args": [
                "-g",
                "main.c",
                "StackLinked.c",
                "-o",
                "main.exe"
            ]
        },
        {
            "label": "Run Stack Exe",
            "type": "shell",
            "command": ".\\main.exe"
        },
        {
            "type": "shell",
            "label": "gcc.exe build active file",
            "command": "C:\\MinGW\\bin\\gcc.exe",
            "args": [
                "-g",
                "${file}",
                "-o",
                "${fileDirname}\\${fileBasenameNoExtension}.exe"
            ],
            "options": {
                "cwd": "C:\\MinGW\\bin"
            }
        }
    ]
}